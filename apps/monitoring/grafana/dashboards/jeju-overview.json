{
  "uid": "jeju-overview",
  "title": "Jeju - Complete Overview",
  "tags": [
    "overview",
    "jeju",
    "summary"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "System Health",
      "type": "stat",
      "gridPos": {
        "h": 3,
        "w": 3,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "up{job='op-node'}",
          "legendFormat": "Node Status"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "value": "1",
              "text": "Healthy"
            },
            {
              "value": "0",
              "text": "Down"
            }
          ],
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Latest Block",
      "type": "stat",
      "gridPos": {
        "h": 3,
        "w": 3,
        "x": 3,
        "y": 0
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT MAX(number) as block FROM block",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "blue"
              }
            ]
          }
        }
      }
    },
