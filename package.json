{
  "name": "jeju-l3",
  "version": "0.1.0",
  "description": "OP-Stack settling on Base with Flashblocks, EigenDA, and complete DeFi stack",
  "type": "module",
  "scripts": {
    "postinstall": "bun run scripts/setup-apps.ts && bun run scripts/ensure-deployment-files.ts || echo 'Setup completed with warnings'",
    "preflight": "bun run scripts/preflight-check.ts",
    "dev": "bun run scripts/preflight-check.ts && bun run scripts/dev.ts",
    "dev:skip-check": "bun run scripts/dev.ts",
    "dev:vendor": "bun run scripts/dev-with-vendor.ts",
    "apps:list": "bun run scripts/list-apps.ts",
    "apps:validate": "bun run scripts/validate-manifests.ts",
    "vendor:list": "bun run scripts/list-apps.ts",
    "wallet": "bun run scripts/show-wallet-config.ts",
    "ports": "bun run scripts/check-ports.ts",
    "cleanup": "bun run scripts/cleanup-processes.ts",
    "build": "bun run scripts/build.ts",
    "test": "bun run scripts/test-all.ts",
    "test:synpress": "bun run scripts/test-all-synpress.ts",
    "test:old": "bun run scripts/test.ts",
    "clean": "bun run scripts/clean.ts",
    "start": "bun run scripts/start.ts",
    "deploy:testnet": "bun run scripts/deploy.ts --network testnet",
    "deploy:mainnet": "bun run scripts/deploy.ts --network mainnet",
    "deploy:cloud": "bun run scripts/deploy-cloud-integration.ts",
    "test:cloud": "bun run scripts/run-cloud-tests.ts",
    "test:cloud:e2e": "bun test tests/e2e/cloud-*.test.ts",
    "setup:cloud:approvers": "bun run scripts/setup-cloud-approvers.ts",
    "verify:cloud": "bun run scripts/verify-cloud-deployment.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.911.0",
    "@eth-optimism/sdk": "^3.2.0",
    "@openzeppelin/contracts": "^5.0.2",
    "@safe-global/safe-core-sdk": "^3.3.5",
    "@synthetixio/v3-contracts": "latest",
    "@uniswap/v4-core": "latest",
    "ethers": "^6.11.1",
    "tar": "^7.5.1",
    "viem": "^2.7.15",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@synthetixio/synpress": "^4.0.0-alpha.7",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "playwright": "^1.56.1",
    "playwright-core": "^1.56.1",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^2.1.0",
    "solhint": "^6.0.1",
    "typescript": "^5.3.3",
    "vitepress": "^1.0.0",
    "yaml-lint": "^1.7.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
