{
  "name": "@jeju/ipfs-pinning-api",
  "version": "1.0.0",
  "description": "Jeju IPFS Pinning Service - Standard IPFS Pinning API with x402 payments and A2A agent",
  "type": "module",
  "scripts": {
    "predev": "bun run db:init && bun run db:migrate",
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun run dist/index.js",
    "test": "bun test src/*.test.ts",
    "db:init": "bun run src/database/init.ts",
    "db:migrate": "bun run src/database/migrate.ts",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@coinbase/x402": "latest",
    "hono": "^4.0.0",
    "ipfs-http-client": "^60.0.0",
    "drizzle-orm": "^0.29.0",
    "postgres": "^3.4.0",
    "viem": "^2.7.15",
    "ethers": "^6.11.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "latest",
    "drizzle-kit": "^0.20.0"
  }
}
