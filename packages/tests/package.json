    "synpress:cache:clear": "rm -rf ../../.synpress-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos/jeju.git",
    "directory": "packages/tests"
  },
  "keywords": [
    "jeju",
    "tests",
    "playwright",
    "synpress",
    "e2e"
  ],
  "dependencies": {
    "@tenkeylabs/dappwright": "^2.11.3"
  },
  "peerDependencies": {
    "@playwright/test": ">=1.40.0",
