{
  "name": "@jejunetwork/config",
  "version": "0.1.0",
  "description": "Configuration files and utilities for Jeju Network",
  "license": "MIT",
  "type": "module",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
