{
  "name": "@jeju/market",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p ${PREDIMARKET_PORT:-4005}",
    "build": "next build",
    "start": "next start -p ${PREDIMARKET_PORT:-4005}",
    "lint": "tsc --noEmit && echo 'Type checking passed'",
    "test": "bun run test:unit && bun run test:e2e",
    "test:unit": "NODE_ENV=test bun test --test-name-pattern='.*' hooks/ tests/unit/",
    "test:e2e": "playwright test tests/e2e/01-homepage.spec.ts tests/e2e/02-market-filters.spec.ts tests/e2e/03-portfolio.spec.ts tests/e2e/04-responsive.spec.ts tests/e2e/complete-flow.spec.ts",
    "test:e2e:wallet": "playwright test tests/e2e/wallet/",
    "test:e2e:all": "playwright test tests/e2e",
    "test:watch": "bun test --watch",
    "setup:test-env": "./scripts/test-env-setup.sh",
    "seed-data": "bun run scripts/seed-testdata.ts",
    "start:test-env": "./scripts/start-test-env.sh"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.1.0",
    "@tanstack/react-query": "^5.56.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "next": "^14.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.0",
    "viem": "^2.21.0",
    "wagmi": "^2.12.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@metamask/providers": "^18.1.0",
    "@playwright/test": "^1.56.1",
    "@synthetixio/synpress": "^4.1.1",
    "@tenkeylabs/dappwright": "^2.11.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.16.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^14.2.0",
    "happy-dom": "^13.6.2",
    "playwright-core": "^1.56.1",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0"
  }
}
