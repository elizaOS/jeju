{
  "name": "@jeju/gateway",
  "version": "1.0.0",
  "type": "module",
  "description": "Gateway Portal - Protocol Infrastructure Hub. Bridge tokens, deploy paymasters, provide ETH liquidity, run nodes, earn rewards. Protocol power = paymaster + liquidity. Supports elizaOS, CLANKER, VIRTUAL, CLANKERMON + any ERC20.",
  "scripts": {
    "dev": "concurrently \"vite\" \"bun src/a2a-server.ts\"",
    "dev:ui": "vite",
    "dev:a2a": "bun src/a2a-server.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "bun run test:unit && bun run test:a2a",
    "test:unit": "bun test src/lib/tests/",
    "test:contracts": "bun test tests/contracts/",
    "test:a2a": "bun test tests/a2a/",
    "test:e2e": "playwright test tests/synpress",
    "test:e2e:smoke": "playwright test tests/synpress/00-basic.spec.ts --headed",
    "test:e2e:headed": "playwright test tests/synpress --headed",
    "test:e2e:debug": "playwright test tests/synpress --debug",
    "test:e2e:old": "playwright test tests/e2e",
    "test:integration": "playwright test tests/integration/",
    "test:watch": "bun test --watch",
    "test:all": "bun run test && bun run test:e2e"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "viem": "^2.38.3",
    "wagmi": "^2.18.1",
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@tanstack/react-query": "^5.90.5",
    "recharts": "^3.3.0",
    "lucide-react": "^0.546.0",
    "express": "^4.19.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@playwright/test": "1.48.2",
    "@synthetixio/synpress": "^4.1.1",
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.1.0",
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "globals": "^15.0.0",
    "typescript-eslint": "^8.0.0",
    "ethers": "^6.15.0",
    "playwright-core": "1.48.2",
    "typescript": "^5.6.0",
    "vite": "^7.1.10"
  }
}
