{
  "$schema": "../../packages/config/jeju-manifest.schema.json",
  "name": "compute",
  "displayName": "Jeju Compute Marketplace",
  "version": "1.0.0",
  "type": "core",
  "description": "Decentralized compute marketplace with SSH/Docker rentals (vast.ai-style), ERC-8004 integration. Run compute nodes, rent GPU resources, stake as providers.",
  "commands": {
    "dev": "bun run dev",
    "build": "bun run build",
    "start": "bun run node",
    "gateway": "bun run gateway",
    "test": "bun run test"
  },
  "ports": {
    "main": 4007,
    "nodeApi": 4008,
    "gateway": 4009,
    "sshProxy": 2222
  },
  "dependencies": ["contracts", "config"],
  "optional": false,
  "enabled": true,
  "autoStart": true,
  "tags": ["compute", "ai", "inference", "staking", "erc-8004", "marketplace", "tee", "gpu"],
  "jns": {
    "name": "compute.jeju",
    "description": "Decentralized Compute Marketplace",
    "url": "https://compute.jeju.network"
  },
  "healthCheck": {
    "url": "http://localhost:4007/health",
    "interval": 30000
  },
  "agent": {
    "enabled": true,
    "a2aEndpoint": "/a2a",
    "tags": ["jeju-app", "compute", "ai", "inference", "marketplace"],
    "metadata": {
      "category": "compute",
      "provider": "jeju-network"
    },
    "trustModels": ["open", "staked"],
    "x402Support": true,
    "jnsName": "compute.jeju"
  }
}

