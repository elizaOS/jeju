{
  "name": "jeju",
  "version": "0.1.0",
  "description": "A modern EVM chain for for agents and humans",
  "type": "module",
  "scripts": {
    "docs:generate": "typedoc --options packages/config/typedoc.json",
    "postinstall": "bun run scripts/setup-apps.ts || echo 'Setup completed with warnings'",
    "preflight": "bun run scripts/preflight-check.ts",
    "dev": "bun run scripts/dev.ts",
    "dev:vendor": "bun run scripts/dev-with-vendor.ts",
    "wallet": "bun run scripts/show-wallet-config.ts",
    "ports": "bun run scripts/check-ports.ts",
    "cleanup": "bun run scripts/cleanup-processes.ts",
    "build": "bun run docs:generate && bun run scripts/build.ts",
    "test": "bun run scripts/test-all.ts",
    "clean": "bun run scripts/clean.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.911.0",
    "@eth-optimism/sdk": "^3.2.0",
    "@openzeppelin/contracts": "^5.0.2",
    "@safe-global/safe-core-sdk": "^3.3.5",
    "@synthetixio/v3-contracts": "latest",
    "@uniswap/v4-core": "latest",
    "ethers": "^6.11.1",
    "tar": "^7.5.1",
    "viem": "^2.7.15",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@account-abstraction/contracts": "^0.8.0",
    "@playwright/test": "^1.56.1",
    "@synthetixio/synpress": "^4.0.0-alpha.7",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "playwright": "^1.56.1",
    "playwright-core": "^1.56.1",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^2.1.0",
    "solhint": "^6.0.1",
    "turbo": "^2.6.3",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.3.3",
    "vitepress": "^1.0.0",
    "yaml-lint": "^1.7.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "ignorePatterns": [
      "node_modules",
      "dist",
      "build",
      "out",
      "lib",
      ".next",
      ".cache",
      "apps",
      "babylon",
      "caliguland2",
      "packages/contracts",
      "packages/kubernetes",
      "packages/terraform",
      "packages/kurtosis",
      "*.config.js",
      "*.config.ts"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": ["warn", {
        "argsIgnorePattern": "^_"
      }],
      "@typescript-eslint/no-explicit-any": "warn",
      "@typescript-eslint/no-var-requires": "off",
      "no-console": "off"
    }
  }
}
