{
  "$schema": "../../packages/config/jeju-manifest.schema.json",
  "name": "indexer",
  "displayName": "Subsquid Indexer",
  "version": "1.0.0",
  "type": "core",
  "description": "Blockchain indexer with GraphQL API",
  "commands": {
    "dev": "bun run dev",
    "build": "bun run build",
    "start": "bun run start"
  },
  "ports": {
    "main": 4350,
    "api": 4350
  },
  "dependencies": ["localnet"],
  "optional": true,
  "enabled": false,
  "autoStart": false,
  "tags": ["indexer", "graphql", "data"],
  "agent": {
    "enabled": true,
    "tags": ["jeju-app", "indexer", "graphql", "data"],
    "metadata": {
      "category": "data",
      "provider": "jeju-network"
    },
    "trustModels": ["open"]
  }
}

