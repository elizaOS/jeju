# Jeju Compute - Environment Variables
# =====================================
# Copy to .env and fill in your values.
# Network config is loaded from @jejunetwork/config.

# === REQUIRED: Provider Wallet ===
PRIVATE_KEY=

# === OPTIONAL: Model Configuration ===
# MODEL_NAME=llama2
# MODEL_BACKEND=ollama  # ollama | mock | llamacpp
# MODEL_ENDPOINT=http://localhost:11434

# === OPTIONAL: Pricing ===
# PRICE_PER_INPUT_TOKEN=1000000000   # 1 gwei
# PRICE_PER_OUTPUT_TOKEN=2000000000  # 2 gwei

# === OVERRIDES (usually not needed) ===
# JEJU_NETWORK=localnet
# COMPUTE_PORT=4007
