{
  "uid": "jeju-blockchain-activity",
  "title": "Jeju - Blockchain Activity",
  "tags": [
    "blockchain",
    "jeju",
    "activity"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "editable": true,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Latest Block Number",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT MAX(number) as block_number FROM block",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Total Transactions (24h)",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 4,
        "y": 0
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT COUNT(*) as total FROM transaction WHERE $__timeFilter(block.timestamp) AND block.id = transaction.block_id",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "blue"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Active Contracts (24h)",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 8,
        "y": 0
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT COUNT(DISTINCT address) as contracts FROM contract WHERE last_seen_at > NOW() - INTERVAL '24 hours'",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "purple"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Unique Active Addresses (24h)",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT COUNT(DISTINCT address) as addresses FROM account WHERE last_seen_block > (SELECT MAX(number) - 7200 FROM block)",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "orange"
              }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Total Gas Used (24h)",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 16,
        "y": 0
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT SUM(gas_used) as total_gas FROM block WHERE timestamp > NOW() - INTERVAL '24 hours'",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              }
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 6,
      "title": "Transactions Per Minute",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  DATE_TRUNC('minute', b.timestamp) as time,\n  COUNT(*) as \"Transactions\"\nFROM transaction t\nJOIN block b ON t.block_id = b.id\nWHERE $__timeFilter(b.timestamp)\nGROUP BY time\nORDER BY time",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 7,
      "title": "Gas Used Per Minute",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  DATE_TRUNC('minute', timestamp) as time,\n  SUM(gas_used) as \"Gas Used\"\nFROM block\nWHERE $__timeFilter(timestamp)\nGROUP BY time\nORDER BY time",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 8,
      "title": "Transaction Status Distribution",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 12
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  status as metric,\n  COUNT(*) as value\nFROM transaction t\nJOIN block b ON t.block_id = b.id\nWHERE b.timestamp > NOW() - INTERVAL '1 hour'\nGROUP BY status",
          "format": "table"
        }
      ],
      "options": {
        "pieType": "donut",
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "value",
            "percent"
          ]
        }
      }
    },
    {
      "id": 9,
      "title": "Average Gas Price (Gwei)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 9,
        "x": 6,
        "y": 12
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  DATE_TRUNC('minute', b.timestamp) as time,\n  AVG(t.gas_price::numeric / 1e9) as \"Avg Gas Price (Gwei)\"\nFROM transaction t\nJOIN block b ON t.block_id = b.id\nWHERE $__timeFilter(b.timestamp) AND t.gas_price IS NOT NULL\nGROUP BY time\nORDER BY time",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Block Size Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 9,
        "x": 15,
        "y": 12
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  timestamp as time,\n  size as \"Block Size (bytes)\"\nFROM block\nWHERE $__timeFilter(timestamp) AND size IS NOT NULL\nORDER BY time",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "color": {
            "mode": "palette-classic"
          },
          "unit": "bytes"
        }
      }
    },
    {
      "id": 11,
      "title": "Recent Blocks",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  number as \"Block\",\n  LEFT(hash, 10) || '...' as \"Hash\",\n  transaction_count as \"Txs\",\n  gas_used as \"Gas Used\",\n  timestamp as \"Time\"\nFROM block\nORDER BY number DESC\nLIMIT 20",
          "format": "table"
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Block",
            "desc": true
          }
        ]
      }
    },
    {
      "id": 12,
      "title": "Recent Transactions",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 20
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  LEFT(t.hash, 10) || '...' as \"Tx Hash\",\n  LEFT(t.from_id, 10) || '...' as \"From\",\n  CASE WHEN t.to_id IS NULL THEN 'Contract Creation' ELSE LEFT(t.to_id, 10) || '...' END as \"To\",\n  (t.value::numeric / 1e18) as \"Value (ETH)\",\n  t.status as \"Status\",\n  b.timestamp as \"Time\"\nFROM transaction t\nJOIN block b ON t.block_id = b.id\nORDER BY b.number DESC, t.transaction_index DESC\nLIMIT 20",
          "format": "table"
        }
      ],
      "options": {
        "showHeader": true
      }
    },
    {
      "id": 13,
      "title": "Daily Activity Trends",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 28
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  date as time,\n  transaction_count as \"Transactions\"\nFROM daily_activity\nWHERE $__timeFilter(date)\nORDER BY time",
          "format": "time_series",
          "refId": "A"
        },
        {
          "rawSql": "SELECT \n  date as time,\n  unique_senders as \"Unique Senders\"\nFROM daily_activity\nWHERE $__timeFilter(date)\nORDER BY time",
          "format": "time_series",
          "refId": "B"
        },
        {
          "rawSql": "SELECT \n  date as time,\n  contract_deployments as \"Contract Deployments\"\nFROM daily_activity\nWHERE $__timeFilter(date)\nORDER BY time",
          "format": "time_series",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Contract Deployments"
            },
            "properties": [
              {
                "id": "custom.axisPlacement",
                "value": "right"
              }
            ]
          }
        ]
      }
    }
  ]
}