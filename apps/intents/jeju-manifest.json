{
  "$schema": "../../packages/config/jeju-manifest.schema.json",
  "name": "intents",
  "displayName": "Open Intents Framework",
  "description": "Cross-chain intent aggregation, solving, and visualization with A2A/MCP interfaces",
  "version": "1.0.0",
  "type": "core",
  "commands": {
    "dev": "bun run aggregator/src/index.ts",
    "build": "bun run build",
    "start": "bun run aggregator/dist/index.js",
    "test": "bun test"
  },
  "ports": {
    "main": 4015,
    "aggregator": 4015,
    "viewer": 4016
  },
  "dependencies": ["contracts", "config"],
  "optional": true,
  "enabled": true,
  "autoStart": false,
  "tags": ["intents", "cross-chain", "oif", "eil", "aggregator", "solver"],
  "healthCheck": {
    "url": "http://localhost:4015/health",
    "interval": 30000
  },
  "agent": {
    "enabled": true,
    "a2aEndpoint": "/a2a",
    "mcpEndpoint": "/mcp",
    "tags": ["jeju-app", "intents", "cross-chain", "oif"],
    "metadata": {
      "category": "infrastructure",
      "provider": "jeju-network"
    },
    "trustModels": ["open"],
    "x402Support": true,
    "jnsName": "intents.jeju"
  }
}

