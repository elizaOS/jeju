{
  "$schema": "../../packages/config/jeju-manifest.schema.json",
  "name": "gateway",
  "displayName": "Gateway Portal",
  "version": "1.0.0",
  "type": "core",
  "description": "Protocol Infrastructure Hub: Bridge tokens, deploy paymasters, provide ETH liquidity, run nodes, earn rewards. Supports elizaOS, CLANKER, VIRTUAL, CLANKERMON + any ERC20. Protocol power = paymaster + liquidity.",
  "commands": {
    "dev": "bun run dev",
    "build": "bun run build",
    "start": "bun run preview"
  },
  "ports": {
    "main": 4001,
    "nodeApi": 4002,
    "a2a": 4003
  },
  "dependencies": ["contracts", "config"],
  "optional": false,
  "enabled": true,
  "autoStart": true,
  "tags": ["protocol", "infrastructure", "paymaster", "nodes", "staking", "liquidity", "elizaOS", "multi-token"],
  "jns": {
    "name": "gateway.jeju",
    "description": "Protocol Infrastructure Hub for Jeju Network",
    "url": "https://gateway.jeju.network"
  },
  "agent": {
    "enabled": true,
    "a2aEndpoint": "http://localhost:4003/a2a",
    "tags": ["jeju-app", "protocol", "infrastructure", "nodes", "staking"],
    "metadata": {
      "category": "infrastructure",
      "provider": "jeju-network"
    },
    "trustModels": ["open"],
    "x402Support": true,
    "jnsName": "gateway.jeju"
  }
}

