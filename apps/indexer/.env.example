# Subsquid Indexer Configuration
# Copy this to .env for local development

# RPC endpoint (required)
RPC_ETH_HTTP=http://localhost:9545

# Starting block (optional, default: 0)
START_BLOCK=0

# Chain ID
CHAIN_ID=1337

# Database configuration
DB_NAME=indexer
DB_PORT=23798
DB_HOST=localhost
DB_USER=postgres
DB_PASS=postgres

# GraphQL port
GQL_PORT=4350
INDEXER_GRAPHQL_PORT=4350

# Logging
LOG_LEVEL=info
