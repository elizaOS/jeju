{
  "entryPoints": [
    "../../apps/bazaar/lib",
    "../../apps/bazaar/hooks",
    "../../apps/bazaar/config",
    "../../apps/crucible/src",
    "../../apps/gateway/src/lib",
    "../../apps/gateway/src/hooks",
    "../../apps/indexer/src",
    "../../apps/leaderboard/src/lib",
    "../../apps/monitoring/server",
    "../../apps/storage/src",
    "../../apps/compute/src"
  ],
  "entryPointStrategy": "expand",
  "tsconfig": "tsconfig.docs.json",
  "out": "../../apps/documentation/api",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "excludePrivate": true,
  "excludeExternals": true,
  "skipErrorChecking": true,
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "notDocumented": false
  },
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "categoryOrder": ["Bazaar", "Crucible", "Gateway", "Indexer", "Leaderboard", "Monitoring", "Storage", "Compute"],
  "groupOrder": ["Functions", "Classes", "Interfaces", "Type Aliases", "Variables"],
  "sort": ["source-order"],
  "exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/__tests__/**",
    "**/tests/**"
  ],
  "cleanOutputDir": true
}
