{
  "uid": "jeju-subsquid-indexer",
  "title": "Subsquid Indexer Overview",
  "tags": [
    "subsquid",
    "indexer",
    "jeju"
  ],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "Processing Status",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "up{job=\"subsquid-processor\"}",
          "legendFormat": "Processor Status",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "value": "1",
              "text": "Running"
            },
            {
              "value": "0",
              "text": "Down"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Last Processed Block",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "subsquid_last_processed_block",
          "legendFormat": "Block Number",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "title": "Database Lag",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "subsquid:database_lag:blocks",
          "legendFormat": "Blocks Behind",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "blocks",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 100,
                "color": "yellow"
              },
              {
                "value": 1000,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "API Status",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "up{job=\"subsquid-api\"}",
          "legendFormat": "API Status",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "value": "1",
              "text": "Online"
            },
            {
              "value": "0",
              "text": "Offline"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Processing Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "subsquid:blocks_processed:rate5m",
          "legendFormat": "Blocks/sec",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "ops",
          "label": "Blocks per Second"
        }
      ]
    },
    {
      "id": 6,
      "title": "API Request Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "subsquid:api_requests:rate5m",
          "legendFormat": "Requests/sec",
          "refId": "A"
        },
        {
          "expr": "subsquid:api_errors:rate5m",
          "legendFormat": "Errors/sec",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Requests per Second"
        }
      ]
    },
    {
      "id": 7,
      "title": "API Latency (P95)",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "subsquid:api_latency:p95",
          "legendFormat": "P95 Latency",
          "refId": "A"
        },
        {
          "expr": "subsquid:api_latency:p99",
          "legendFormat": "P99 Latency",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Response Time"
        }
      ]
    },
    {
      "id": 8,
      "title": "Database Connections",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "targets": [
        {
          "expr": "pg_stat_database_numbackends{datname=\"indexer\"}",
          "legendFormat": "Active Connections",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Connections"
        }
      ]
    }
  ]
}