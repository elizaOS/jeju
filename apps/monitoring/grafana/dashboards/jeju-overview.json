    {
      "id": 9,
      "title": "Chain Activity - Last 24 Hours",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 3
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  DATE_TRUNC('hour', b.timestamp) as time,\n  COUNT(DISTINCT t.hash) as \"Transactions\"\nFROM transaction t\nJOIN block b ON t.block_id = b.id\nWHERE b.timestamp > NOW() - INTERVAL '24 hours'\nGROUP BY time\nORDER BY time",
          "format": "time_series",
          "refId": "A"
        },
        {
          "rawSql": "SELECT \n  DATE_TRUNC('hour', timestamp) as time,\n  COUNT(*) as \"Blocks\"\nFROM block\nWHERE timestamp > NOW() - INTERVAL '24 hours'\nGROUP BY time\nORDER BY time",
          "format": "time_series",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Block Production & Gas Usage",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 3
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  DATE_TRUNC('hour', timestamp) as time,\n  AVG(gas_used) as \"Avg Gas Used\"\nFROM block\nWHERE timestamp > NOW() - INTERVAL '24 hours'\nGROUP BY time\nORDER BY time",
          "format": "time_series",
          "refId": "A"
        },
        {
          "rawSql": "SELECT \n  DATE_TRUNC('hour', timestamp) as time,\n  AVG(transaction_count) as \"Avg Tx per Block\"\nFROM block\nWHERE timestamp > NOW() - INTERVAL '24 hours'\nGROUP BY time\nORDER BY time",
          "format": "time_series",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 11,
      "title": "Top Event Types (24h)",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 11
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  event_name as metric,\n  COUNT(*) as value\nFROM decoded_event de\nWHERE timestamp > NOW() - INTERVAL '24 hours'\nGROUP BY event_name\nORDER BY value DESC\nLIMIT 10",
          "format": "table"
        }
      ],
      "options": {
        "pieType": "donut",
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "value",
            "percent"
          ]
        }
      }
    },
    {
      "id": 12,
      "title": "Contract Activity Distribution",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 11
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  COALESCE(contract_type, 'UNKNOWN') as metric,\n  COUNT(*) as value\nFROM contract\nWHERE last_seen_at > NOW() - INTERVAL '24 hours'\nGROUP BY contract_type\nORDER BY value DESC",
          "format": "table"
        }
      ],
      "options": {
        "pieType": "donut",
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "value",
            "percent"
          ]
        }
      }
    },
    {
      "id": 13,
      "title": "Token Standard Activity",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 11
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  token_standard as metric,\n  COUNT(*) as value\nFROM token_transfer\nWHERE timestamp > NOW() - INTERVAL '24 hours'\nGROUP BY token_standard",
          "format": "table"
        }
      ],
      "options": {
        "pieType": "donut",
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "value",
            "percent"
          ]
        }
      }
    },
    {
      "id": 14,
      "title": "Prediction Markets Activity",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 19
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  DATE_TRUNC('hour', mt.timestamp) as time,\n  COUNT(*) as \"Trades\"\nFROM market_trade mt\nWHERE $__timeFilter(mt.timestamp)\nGROUP BY time\nORDER BY time",
          "format": "time_series",
          "refId": "A"
        },
        {
          "rawSql": "SELECT \n  DATE_TRUNC('hour', created_at) as time,\n  COUNT(*) as \"New Markets\"\nFROM prediction_market\nWHERE $__timeFilter(created_at)\nGROUP BY time\nORDER BY time",
          "format": "time_series",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 15,
      "title": "Active Users & Contracts",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 19
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "SELECT \n  DATE_TRUNC('hour', b.timestamp) as time,\n  COUNT(DISTINCT t.from_id) as \"Unique Senders\"\nFROM transaction t\nJOIN block b ON t.block_id = b.id\nWHERE $__timeFilter(b.timestamp)\nGROUP BY time\nORDER BY time",
          "format": "time_series",
          "refId": "A"
        },
        {
          "rawSql": "SELECT \n  DATE_TRUNC('hour', b.timestamp) as time,\n  COUNT(DISTINCT l.address_id) as \"Active Contracts\"\nFROM log l\nJOIN block b ON l.block_id = b.id\nWHERE $__timeFilter(b.timestamp)\nGROUP BY time\nORDER BY time",
          "format": "time_series",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 16,
      "title": "Recent Activity Summary",
      "type": "table",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 27
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres-indexer"
      },
      "targets": [
        {
          "rawSql": "WITH recent_activity AS (\n  SELECT \n    'Transaction' as \"Type\",\n    LEFT(t.hash, 15) || '...' as \"ID\",\n    'From: ' || LEFT(t.from_id, 10) || '... To: ' || COALESCE(LEFT(t.to_id, 10), 'Contract Creation') || '...' as \"Details\",\n    b.timestamp as \"Time\"\n  FROM transaction t\n  JOIN block b ON t.block_id = b.id\n  WHERE b.timestamp > NOW() - INTERVAL '1 hour'\n  ORDER BY b.timestamp DESC\n  LIMIT 10\n)\nSELECT * FROM recent_activity\nORDER BY \"Time\" DESC",
          "format": "table"
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Time",
            "desc": true
          }
        ]
      }
    },
    {
      "id": 17,
      "title": "Indexer Processing Status",
