        }
      }
    },
    {
      "id": 20,
      "title": "Flashblocks Latency (P95)",
      "type": "gauge",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 37
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(op_node_flashblocks_subblock_latency_ms_bucket[5m]))",
          "legendFormat": "P95 Latency (ms)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 100,
                "color": "yellow"
              },
              {
                "value": 250,
                "color": "red"
              }
            ]
          },
          "unit": "ms",
          "max": 500,
          "min": 0
        }
      },
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    }
  ]
}