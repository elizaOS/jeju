  "nodeStacks": {
    "ethereum": {
      "description": "Standard Ethereum L1 (Geth + Lighthouse)",
      "components": ["geth-l1", "lighthouse"]
    },
    "op-stack": {
      "description": "Optimism Stack L2 (op-geth + op-node)",
      "components": ["op-geth", "op-node"],
      "requiresL1": true
    },
    "nitro": {
      "description": "Arbitrum Nitro L2",
      "components": ["nitro-node"],
      "requiresL1": true,
      "requiresBeacon": true
    },
    "bsc": {
      "description": "BNB Smart Chain (modified Geth)",
      "components": ["bsc-geth"]
    },
    "polygon": {
      "description": "Polygon PoS (Bor + Heimdall)",
      "components": ["bor", "heimdall"],
      "requiresL1": true
    }
  },
  
  "deploymentPhases": {
    "phase1-testnet-core": {
      "description": "Core testnet infrastructure",
      "chains": [
        "ethereum-sepolia",
        "jeju-testnet"
      ],
      "estimatedCost": "$200-400/month"
    },
    "phase2-testnet-l2s": {
      "description": "L2 testnet full nodes",
      "chains": [
        "base-sepolia",
        "optimism-sepolia",
        "arbitrum-sepolia"
      ],
      "estimatedCost": "$400-600/month"
    },
    "phase3-testnet-alt": {
      "description": "Alternative L1 testnets",
      "chains": [
        "bsc-testnet"
      ],
      "estimatedCost": "$200-300/month"
    },
    "phase4-mainnet-core": {
      "description": "Mainnet L1 infrastructure",
      "chains": [
        "ethereum-mainnet",
        "jeju-mainnet"
      ],
      "estimatedCost": "$800-1200/month"
    },
    "phase5-mainnet-l2s": {
      "description": "Mainnet L2 full nodes",
      "chains": [
        "base-mainnet",
        "optimism-mainnet",
        "arbitrum-one"
      ],
      "estimatedCost": "$1500-2500/month"
    },
    "phase6-mainnet-alt": {
      "description": "Alternative mainnet chains",
      "chains": [
        "bsc-mainnet",
        "polygon-pos"
      ],
      "estimatedCost": "$800-1200/month"
    }
  },
  
